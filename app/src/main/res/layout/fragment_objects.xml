<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragment.ObjectsFragment">

    <androidx.camera.view.PreviewView
        android:id="@+id/pvCameraPreview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <pl.edu.pwr.lab3.i242487.view.GraphicOverlay
        android:id="@+id/goGraphicOverlay"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/pvCameraPreview"
        app:layout_constraintEnd_toEndOf="@+id/pvCameraPreview"
        app:layout_constraintStart_toStartOf="@+id/pvCameraPreview"
        app:layout_constraintTop_toTopOf="@+id/pvCameraPreview"
        tools:visibility="invisible" />

</androidx.constraintlayout.widget.ConstraintLayout>